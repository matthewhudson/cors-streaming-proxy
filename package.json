    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "mocha": "3.1.2",
    "nodemon": "^1.19.1",
    "open": "^6.4.0",
    "supertest": "2.0.1"
  },
  "dependencies": {
    "request": "2.81.0",
